    "apollo-link-ws": "^1.0.4",
    "apollo-logger": "0.1.1",
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^4.0.0-alpha.3",
    "apollo-utilities": "^1.0.4",
    "axios": "^0.17.1",
    "babel-polyfill": "6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chalk": "2.3.0",
    "compression": "1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "5.1.3",
    "dotenv": "^4.0.0",
    "enhanced-resolve": "^4.0.0-beta.4",
    "express": "4.16.2",
    "fontfaceobserver": "2.0.13",
    "graphql": "^0.12.3",
    "graphql-auth": "^0.2.0",
    "graphql-iso-date": "^3.3.0",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.18.0",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.3.1",
    "immutable": "3.8.2",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.5.6",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "mongoose": "^5.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-ldapauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "pleeease-filters": "^4.0.0",
    "pretty-error": "^2.1.1",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-amap": "^1.2.0",
    "react-apollo": "^2.0.4",
    "react-bootstrap": "^0.32.0",
    "react-cookie": "^2.1.2",
