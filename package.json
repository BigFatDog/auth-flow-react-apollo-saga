{
  "name": "auth-flow-react-apollo-saga",
  "version": "1.0.5",
  "description": "Full stack login/register flow with React, Apollo, Redux, Redux-saga and MongoDB.",
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFatDog/auth-flow-react-apollo-saga.git"
  },
